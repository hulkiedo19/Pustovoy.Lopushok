<Application x:Class="Pustovoy.Lopushok.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Pustovoy.Lopushok"
             StartupUri="Presentation/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="ColorPrimary">#FFF</SolidColorBrush>
            <SolidColorBrush x:Key="ColorSecondary">#CEFFF9</SolidColorBrush>
            <SolidColorBrush x:Key="ColorAccent">#00CC76</SolidColorBrush>
            <SolidColorBrush x:Key="ColorSelect">#FF0000</SolidColorBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>
